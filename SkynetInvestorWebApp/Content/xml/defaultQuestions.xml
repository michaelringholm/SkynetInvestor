        <Options>
          <RadioButtonOption>
            <Score>10</Score>
            <Title>Worsen a bit</Title>
            <Value>WORSEN</Value>
            <Checked>true</Checked>
          </RadioButtonOption>
          <RadioButtonOption>
            <Score>20</Score>
            <Title>The same</Title>
            <Value>SAME</Value>
            <Checked>false</Checked>
          </RadioButtonOption>
          <RadioButtonOption>
            <Score>30</Score>
            <Title>Improve a little</Title>
            <Value>IMPROVE_LITTLE</Value>
            <Checked>false</Checked>
          </RadioButtonOption>
          <RadioButtonOption>
            <Score>40</Score>
