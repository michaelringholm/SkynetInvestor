<?xml version="1.0"?>
<ArrayOfQuestionSection xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <QuestionSection>
    <Title>Financial situation</Title>
    <Questions>
      <Question xsi:type="RadioButtonQuestion">
        <Id>1</Id>
        <Description>How old are you?</Description>
        <Score>0</Score>
        <Options>
          <RadioButtonOption>
            <Score>40</Score>
            <Title>Under 30yrs</Title>
            <Value>30</Value>
            <Checked>true</Checked>
          </RadioButtonOption>
          <RadioButtonOption>
            <Score>30</Score>
            <Title>Between 30-45yrs</Title>
            <Value>30-45</Value>
            <Checked>false</Checked>
          </RadioButtonOption>
          <RadioButtonOption>
            <Score>20</Score>
            <Title>Between 45-65yrs</Title>
            <Value>45-60</Value>
            <Checked>false</Checked>
          </RadioButtonOption>
          <RadioButtonOption>
            <Score>10</Score>
            <Title>65+</Title>
            <Value>65+</Value>
            <Checked>false</Checked>
          </RadioButtonOption>
        </Options>
        <GroupName>AGE</GroupName>
      </Question>
      <Question xsi:type="RadioButtonQuestion">
        <Id>2</Id>
        <Description>How do you expect your future income to evolve over the next 5-7 years?</Description>
