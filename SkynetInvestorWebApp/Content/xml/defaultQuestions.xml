            <Value>IMPROVE_MUCH</Value>
            <Checked>false</Checked>
          </RadioButtonOption>
        </Options>
        <GroupName>FUTURE_INCOME</GroupName>
      </Question>
      <Question xsi:type="RadioButtonQuestion">
        <Id>3</Id>
        <Description>Divide your liabilities by assets and tell us if the result is:</Description>
        <Score>0</Score>
        <Options>
          <RadioButtonOption>
            <Score>10</Score>
            <Title>&lt;10%</Title>
            <Value>10</Value>
            <Checked>true</Checked>
          </RadioButtonOption>
          <RadioButtonOption>
            <Score>20</Score>
            <Title>10-25%</Title>
            <Value>10-25</Value>
            <Checked>false</Checked>
          </RadioButtonOption>
          <RadioButtonOption>
            <Score>30</Score>
            <Title>25%-40%</Title>
            <Value>25-40</Value>
            <Checked>false</Checked>
          </RadioButtonOption>
          <RadioButtonOption>
            <Score>40</Score>
            <Title>40%-60%</Title>
            <Value>40-60</Value>
            <Checked>false</Checked>
          </RadioButtonOption>
        </Options>
        <GroupName>ASSETS_AND_LIABILITIES</GroupName>
      </Question>
    </Questions>
